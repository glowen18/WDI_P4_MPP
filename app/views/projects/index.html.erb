
<div class="container-fluid">

  <table class = "table table-hover">

    <thead>
      <tr>
        <th>Title</th>
      </tr>
    </thead>

    <tbody>
      <% @projects.each do |project| %>
        <tr>
        <td><%= link_to project.title, project_path(project.id) %></td>

      </tr>
      <% end %>
    </tbody>
  </table>

<hr>

    <%= link_to "Create New Project", new_project_path, class: "btn btn-info" %>


</div>
